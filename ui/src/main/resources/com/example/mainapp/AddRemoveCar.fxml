<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="508.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.mainapp.AddRemoveCarController">
   <TextField fx:id="txtCarId" layoutX="45.0" layoutY="363.0" prefHeight="27.0" prefWidth="178.0" promptText="Car-ID"
              style="-fx-border-color: gray;">
      <font>
         <Font size="14.0"/>
      </font>
   </TextField>
   <Button fx:id="btnRemove" layoutX="45.0" layoutY="410.0" mnemonicParsing="false" onAction="#RemoveCarClicked"
           prefHeight="26.0" prefWidth="178.0" style="-fx-border-color: gray;" text="Remove">
      <font>
         <Font size="14.0"/>
      </font>
   </Button>
   <Label fx:id="labelAdd" layoutX="24.0" layoutY="14.0" prefHeight="30.0" prefWidth="220.0"/>
   <Label fx:id="labelRemove" layoutX="24.0" layoutY="325.0" prefHeight="31.0" prefWidth="220.0"/>
   <VBox alignment="CENTER" layoutX="44.0" layoutY="52.0" prefHeight="176.0" prefWidth="178.0">
      <TextField fx:id="txtBranch" promptText="Branch" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font name="Cambria" size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="txtColor" promptText="Color" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font name="Cambria" size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="txtYear" promptText="Year" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="txtModel" promptText="Model" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="txtPrice" promptText="Price" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="txtType" promptText="Type" style="-fx-border-color: gray; -fx-alignment: center;">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
   </VBox>
   <Button fx:id="btnAdd" layoutX="44.0" layoutY="269.0" mnemonicParsing="false" onAction="#AddCarClicked"
           prefHeight="28.0" prefWidth="178.0" style="-fx-border-color: gray;" text="Add" textAlignment="CENTER">
      <font>
         <Font name="Cambria" size="14.0"/>
      </font>
   </Button>
   <ImageView fitHeight="48.0" fitWidth="58.0" layoutX="15.0" layoutY="443.0" onMouseClicked="#BackClicked"
              pickOnBounds="true" preserveRatio="true">
      <Image url="@../../../png/back.png"/>
   </ImageView>
</AnchorPane>
