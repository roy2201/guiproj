<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="408.0" prefWidth="527.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mainapp.DCController">
   <children>
      <ListView fx:id="propListView" layoutX="318.0" layoutY="34.0" prefHeight="268.0" prefWidth="194.0" style="-fx-border-color: black;" />
      <Label fx:id="errorLabel" layoutX="30.0" layoutY="160.0" prefHeight="44.0" prefWidth="255.0" textFill="RED">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="55.0" fitWidth="90.0" layoutX="7.0" layoutY="344.0" onMouseClicked="#backAction2" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../png/back.png" />
         </image>
      </ImageView>
      <HBox alignment="TOP_CENTER" layoutX="14.0" layoutY="267.0" prefHeight="34.0" prefWidth="269.0" style="-fx-border-color: white;">
         <children>
            <Label prefHeight="38.0" prefWidth="83.0" text="days to rent" textFill="#4b4646">
               <font>
                  <Font name="Cambria" size="14.0" />
               </font>
            </Label>
            <Spinner fx:id="daysToRentSpinner" editable="true" prefHeight="39.0" prefWidth="145.0" />
         </children>
      </HBox>
      <HBox layoutX="318.0" layoutY="322.0" prefHeight="63.0" prefWidth="174.0">
